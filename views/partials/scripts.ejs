<!-- Async style loading -->
<link rel="preload" as="style" href="/stylesheets/main.css?v=<%= pkg.version %>">
<script>
  document.addEventListener("DOMContentLoaded", function () {
    document.querySelector('[data-selector="status"]').setAttribute('data-status', 'loaded');
    document.querySelectorAll('[rel="preload"][as="style"]').forEach(function preload(link) {
      link.setAttribute('rel', 'stylesheet');
    })
  });
</script>

